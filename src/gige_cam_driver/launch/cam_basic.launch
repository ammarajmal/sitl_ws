<?xml version="1.0"?>
<launch>
    <arg name="nuc_name" default="" />
    <arg name="device_id" default="" />
    <arg name="calib_file" default="" />
    <node   name="$(arg nuc_name)"
            pkg="gige_cam_driver"
            type="camera_node.py"
            output="log"
        >
        <param name="device_id" value="$(arg device_id)"/>
        <param name="camera_manager" value="$(arg nuc_name)"/>
        <param name="calibration_file" value="$(find gige_cam_driver)/config/camera_info_$(arg calib_file).yaml"/>
    </node>


</launch>